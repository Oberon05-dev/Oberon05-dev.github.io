Dialogue: 0,0:00:03.88,0:00:06.44,NORMAL,,0000,0000,0000,,Nazywam się Naruto Uzumaki.
Dialogue: 0,0:00:06.44,0:00:10.21,NORMAL,,0000,0000,0000,,Moje codzienne życie wypełniają misje,\Nktóre zlecają mi władze wioski,
Dialogue: 0,0:00:10.21,0:00:11.90,NORMAL,,0000,0000,0000,,jednak dzisiaj mam wolne,
Dialogue: 0,0:00:11.90,0:00:16.11,NORMAL,,0000,0000,0000,,dlatego zorganizowaliśmy grilla\Ndla geninów z wioski!
Dialogue: 0,0:00:20.27,0:00:25.37,NORMAL,,0000,0000,0000,,Będąc tak daleko od siebie,\Nciężko wam będzie się lepiej poznać.
Dialogue: 0,0:00:28.50,0:00:31.37,NORMAL,,0000,0000,0000,,Mądry Polak po szkodzie.
Dialogue: 0,0:00:31.37,0:00:34.74,NORMAL,,0000,0000,0000,,Jedz też trochę warzyw!
Dialogue: 0,0:00:35.20,0:00:37.11,NORMAL,,0000,0000,0000,,Przecież Sasuke je tylko mięso!
Dialogue: 0,0:00:37.11,0:00:38.16,NORMAL,,0000,0000,0000,,Sasuke może.
Dialogue: 0,0:00:38.16,0:00:39.63,NORMAL,,0000,0000,0000,,Niby dlaczego?
Dialogue: 0,0:00:42.91,0:00:44.76,NORMAL,,0000,0000,0000,,Widzieliście w pobliżu jakiś sklep?
Dialogue: 0,0:00:45.25,0:00:48.38,NORMAL,,0000,0000,0000,,Mijaliśmy automaty z napojami.
Dialogue: 0,0:00:48.38,0:00:49.48,NORMAL,,0000,0000,0000,,Rozumiem.
Dialogue: 0,0:00:50.26,0:00:52.40,NORMAL,,0000,0000,0000,,Idę coś sobie kupić.
Dialogue: 0,0:00:54.07,0:00:56.58,NORMAL,,0000,0000,0000,,Teraz możemy zaszaleć.
Dialogue: 0,0:01:03.78,0:01:05.90,NORMAL,,0000,0000,0000,,Zostaw też trochę dla mistrza!
Dialogue: 0,0:01:05.90,0:01:09.04,NORMAL,,0000,0000,0000,,Nie trzeba było\Nnas tutaj zostawiać.
Dialogue: 0,0:01:09.04,0:01:11.03,NORMAL,,0000,0000,0000,,Podaj sos.
Dialogue: 0,0:01:11.03,0:01:14.26,NORMAL,,0000,0000,0000,,Utrapienie z tobą.
Dialogue: 0,0:01:19.02,0:01:20.38,NORMAL,,0000,0000,0000,,Co to takiego?
Dialogue: 0,0:01:22.95,0:01:25.18,NORMAL,,0000,0000,0000,,Co ty wyprawiasz?
Dialogue: 0,0:01:30.21,0:01:31.95,NORMAL,,0000,0000,0000,,Co to takiego?
Dialogue: 0,0:01:32.16,0:01:37.10,NORMAL,,0000,0000,0000,,Naruto, dżin i trzy życzenia.
Dialogue: 0,0:01:39.88,0:01:42.20,NORMAL,,0000,0000,0000,,Coś jest w środku.
Dialogue: 0,0:01:49.84,0:01:53.83,NORMAL,,0000,0000,0000,,Po tysiącu lat znów wolny!
Dialogue: 0,0:01:53.83,0:01:55.78,NORMAL,,0000,0000,0000,,Świat już na mnie czeka!
Dialogue: 0,0:01:55.78,0:01:58.10,NORMAL,,0000,0000,0000,,Czy wiecie, kim jestem?
Dialogue: 0,0:01:58.10,0:02:03.15,NORMAL,,0000,0000,0000,,Nie znacie odpowiedzi?\NWięc wam ją zdradzę!
Dialogue: 0,0:02:03.15,0:02:05.76,NORMAL,,0000,0000,0000,,Jestem legendarnym\Ndżinem z lampy!
Dialogue: 0,0:02:05.76,0:02:10.07,NORMAL,,0000,0000,0000,,Spełnię wasze trzy życzenia!
Dialogue: 0,0:02:10.07,0:02:12.02,NORMAL,,0000,0000,0000,,Życzcie sobie,\Nczego chcecie.
Dialogue: 0,0:02:12.02,0:02:13.43,NORMAL,,0000,0000,0000,,Naruto, podaj sos.
Dialogue: 0,0:02:14.51,0:02:17.26,NORMAL,,0000,0000,0000,,Słuchajcie mnie!
Dialogue: 0,0:02:17.26,0:02:19.42,NORMAL,,0000,0000,0000,,Kto by wierzył w takie bajki?
Dialogue: 0,0:02:19.42,0:02:21.53,NORMAL,,0000,0000,0000,,Nie patrz mu w oczy!
Dialogue: 0,0:02:21.53,0:02:23.38,NORMAL,,0000,0000,0000,,Wygląda podejrzanie.
Dialogue: 0,0:02:23.73,0:02:25.19,NORMAL,,0000,0000,0000,,Nie jestem podejrzany!
Dialogue: 0,0:02:25.19,0:02:27.84,NORMAL,,0000,0000,0000,,Naprawdę jestem dżinem!
Dialogue: 0,0:02:27.84,0:02:29.15,NORMAL,,0000,0000,0000,,Naprawdę?
Dialogue: 0,0:02:29.15,0:02:31.99,NORMAL,,0000,0000,0000,,Mówiłam ci,\Nżebyś z nim nie gadał!
Dialogue: 0,0:02:31.99,0:02:33.40,NORMAL,,0000,0000,0000,,Nie kłamię!
Dialogue: 0,0:02:33.40,0:02:35.63,NORMAL,,0000,0000,0000,,Dżiny nie potrafią kłamać!
Dialogue: 0,0:02:35.63,0:02:39.07,NORMAL,,0000,0000,0000,,Wyczaruj przekąsek na grilla\Ndla dziesięciu osób!
Dialogue: 0,0:02:39.07,0:02:43.38,NORMAL,,0000,0000,0000,,Twe życzenie jest\Ndla mnie rozkazem!
Dialogue: 0,0:02:45.86,0:02:48.04,NORMAL,,0000,0000,0000,,To najprawdziwszy dżin.
Dialogue: 0,0:02:48.04,0:02:51.75,NORMAL,,0000,0000,0000,,Panie dżinie,\Njest pan wspaniały!
Dialogue: 0,0:02:51.75,0:02:54.63,NORMAL,,0000,0000,0000,,Nie zaprzeczam!
Dialogue: 0,0:02:54.63,0:02:58.49,NORMAL,,0000,0000,0000,,Nadal macie dwa życzenia!
Dialogue: 0,0:02:59.72,0:03:02.83,NORMAL,,0000,0000,0000,,Czego sobie życzycie?
Dialogue: 0,0:03:02.83,0:03:04.48,NORMAL,,0000,0000,0000,,Co robimy?
Dialogue: 0,0:03:04.48,0:03:06.84,NORMAL,,0000,0000,0000,,Nie możemy chcieć\Nczegoś prostego.
Dialogue: 0,0:03:06.84,0:03:08.62,NORMAL,,0000,0000,0000,,Poprośmy o pieniądze!
Dialogue: 0,0:03:08.62,0:03:11.42,NORMAL,,0000,0000,0000,,Sakura, pieniądze to nie wszystko.
Dialogue: 0,0:03:11.42,0:03:11.70,NORMAL,,0000,0000,0000,,- Po prostu myślę praktycznie!
Dialogue: 0,0:03:11.70,0:03:13.40,NORMAL,,0000,0000,0000,,- Zdecydowaliście już?\N- Po prostu myślę praktycznie!
Dialogue: 0,0:03:13.40,0:03:15.40,NORMAL,,0000,0000,0000,,- Zdecydowaliście już?\N- Więc czego ty byś chciał?
Dialogue: 0,0:03:15.40,0:03:16.87,NORMAL,,0000,0000,0000,,Muszę pomyśleć.
Dialogue: 0,0:03:16.87,0:03:19.04,NORMAL,,0000,0000,0000,,Naruto, ty odpadasz.
Dialogue: 0,0:03:19.04,0:03:19.92,NORMAL,,0000,0000,0000,,Dlaczego?!
Dialogue: 0,0:03:19.92,0:03:21.43,NORMAL,,0000,0000,0000,,Wykorzystałeś już swoje życzenie!
Dialogue: 0,0:03:21.43,0:03:22.95,NORMAL,,0000,0000,0000,,Dla testów!
Dialogue: 0,0:03:22.95,0:03:25.84,NORMAL,,0000,0000,0000,,Poproszę o kolejne życzenie.
Dialogue: 0,0:03:25.84,0:03:28.52,NORMAL,,0000,0000,0000,,Przymknij się,\Npoczekaj jeszcze chwilę.
Dialogue: 0,0:03:28.52,0:03:31.70,NORMAL,,0000,0000,0000,,Dobrze, wasze życzenie\Njest dla mnie rozkazem.
Dialogue: 0,0:03:32.02,0:03:33.90,NORMAL,,0000,0000,0000,,I czas na ostatnie życzenie!
Dialogue: 0,0:03:35.06,0:03:37.89,NORMAL,,0000,0000,0000,,To nie było życzenie!
Dialogue: 0,0:03:37.89,0:03:40.51,NORMAL,,0000,0000,0000,,Nie mogę łamać przepisów.
Dialogue: 0,0:03:40.51,0:03:41.97,NORMAL,,0000,0000,0000,,Dlaczego?!
Dialogue: 0,0:03:43.00,0:03:47.76,NORMAL,,0000,0000,0000,,Proszę przeczytać punkt trzeci\Ninstrukcji obsługi.
Dialogue: 0,0:03:47.76,0:03:49.34,NORMAL,,0000,0000,0000,,On ma rację.
Dialogue: 0,0:03:50.42,0:03:53.88,NORMAL,,0000,0000,0000,,Jakie jest wasze ostatnie życzenie?
Dialogue: 0,0:03:53.88,0:03:55.46,NORMAL,,0000,0000,0000,,Kretyn!
Dialogue: 0,0:03:55.46,0:03:57.11,NORMAL,,0000,0000,0000,,Coś ty najlepszego zrobił?!
Dialogue: 0,0:03:58.94,0:04:01.32,NORMAL,,0000,0000,0000,,Został jedno życzenie,\Nco robimy?
Dialogue: 0,0:04:03.28,0:04:06.81,NORMAL,,0000,0000,0000,,Zdaje się, że musicie to sobie przemyśleć,\Nwięc poczekam grzecznie w butelce.
Dialogue: 0,0:04:06.81,0:04:08.34,NORMAL,,0000,0000,0000,,Proszę tak zrobić.
Dialogue: 0,0:04:17.16,0:04:20.22,NORMAL,,0000,0000,0000,,Przestań nam przeszkadzać.
Dialogue: 0,0:04:20.22,0:04:22.06,NORMAL,,0000,0000,0000,,Co to miało znaczyć?!
Dialogue: 0,0:04:22.06,0:04:24.73,NORMAL,,0000,0000,0000,,To ja znalazłem tę butelkę!
Dialogue: 0,0:04:24.73,0:04:28.04,NORMAL,,0000,0000,0000,,I przy okazji popsułeś\Ndwa życzenia!
Dialogue: 0,0:04:28.04,0:04:29.64,NORMAL,,0000,0000,0000,,Więc lepiej się już przymknij!
Dialogue: 0,0:04:29.64,0:04:33.83,NORMAL,,0000,0000,0000,,Czego ty byś chciała?
Dialogue: 0,0:04:34.58,0:04:37.76,NORMAL,,0000,0000,0000,,Coś bardzo cennego.
Dialogue: 0,0:04:37.77,0:04:41.73,NORMAL,,0000,0000,0000,,Lepiej nie mówić,\Nże chcę, by Sasuke mnie pokochał.
Dialogue: 0,0:04:42.29,0:04:43.13,NORMAL,,0000,0000,0000,,Podziękował!
Dialogue: 0,0:04:43.13,0:04:45.16,NORMAL,,0000,0000,0000,,Zostanę hokage!
Dialogue: 0,0:04:45.16,0:04:45.96,NORMAL,,0000,0000,0000,,Naruto!
Dialogue: 0,0:04:45.96,0:04:47.84,NORMAL,,0000,0000,0000,,Stój, diable jeden!
Dialogue: 0,0:04:54.00,0:04:54.96,NORMAL,,0000,0000,0000,,Widzieliście?
Dialogue: 0,0:04:54.96,0:04:56.01,NORMAL,,0000,0000,0000,,Tak.
Dialogue: 0,0:04:56.01,0:04:58.63,NORMAL,,0000,0000,0000,,Jeśli to prawda, nie możemy\Nprzepuścić takiej okazji.
Dialogue: 0,0:04:59.87,0:05:02.00,NORMAL,,0000,0000,0000,,Butelka z dżinem będzie moja!
Dialogue: 0,0:05:02.76,0:05:03.90,NORMAL,,0000,0000,0000,,Kiba!
Dialogue: 0,0:05:03.90,0:05:05.63,NORMAL,,0000,0000,0000,,Nie możemy zostać w tyle!
Dialogue: 0,0:05:07.56,0:05:09.12,NORMAL,,0000,0000,0000,,To moja szansa!
Dialogue: 0,0:05:10.87,0:05:12.92,NORMAL,,0000,0000,0000,,To takie wkurzające.
Dialogue: 0,0:05:18.52,0:05:19.62,NORMAL,,0000,0000,0000,,Naruto.
Dialogue: 0,0:05:28.21,0:05:31.22,NORMAL,,0000,0000,0000,,Oddaj mi grzecznie\Ntę butelkę.
Dialogue: 0,0:05:31.22,0:05:34.56,NORMAL,,0000,0000,0000,,Chyba śnisz!\NZamierzam zostać hokage!
Dialogue: 0,0:05:34.56,0:05:36.65,NORMAL,,0000,0000,0000,,Wiedziałem,\Nże to powiesz.
Dialogue: 0,0:05:37.58,0:05:39.32,NORMAL,,0000,0000,0000,,Idziemy, Akamaru!
Dialogue: 0,0:05:39.32,0:05:42.02,NORMAL,,0000,0000,0000,,SZTUKA BESTII: KLONY BESTII!\NKIEŁ ZA KŁEM!
Dialogue: 0,0:05:50.87,0:05:52.42,NORMAL,,0000,0000,0000,,Co on wyprawia?
Dialogue: 0,0:05:59.16,0:06:01.44,NORMAL,,0000,0000,0000,,Oddaj butelkę!
Dialogue: 0,0:06:01.44,0:06:02.72,NORMAL,,0000,0000,0000,,Ty też?!
Dialogue: 0,0:06:08.55,0:06:11.87,NORMAL,,0000,0000,0000,,Dlaczego tak ci zależy?\NTo nie w twoim stylu.
Dialogue: 0,0:06:11.87,0:06:14.80,NORMAL,,0000,0000,0000,,Czego tak bardzo pragniesz?
Dialogue: 0,0:06:14.80,0:06:17.04,NORMAL,,0000,0000,0000,,I tak byś nie zrozumiał.
Dialogue: 0,0:06:19.99,0:06:22.04,NORMAL,,0000,0000,0000,,SZTUKA OGNIA:\NNISZCZĄCE KULE!
Dialogue: 0,0:06:29.35,0:06:31.37,NORMAL,,0000,0000,0000,,Teraz moja kolej!
Dialogue: 0,0:06:31.37,0:06:32.38,NORMAL,,0000,0000,0000,,Przestańcie!
Dialogue: 0,0:06:34.84,0:06:36.57,NORMAL,,0000,0000,0000,,Co wy robicie?
Dialogue: 0,0:06:37.00,0:06:42.13,NORMAL,,0000,0000,0000,,Czy naprawdę tak wam zależy na tym\Nżyczeniu, by walczyć między sobą?
Dialogue: 0,0:06:42.83,0:06:51.45,NORMAL,,0000,0000,0000,,Jesteśmy drużyną, więc zastanówcie się,\Nczy tak postępują przyjaciele!
Dialogue: 0,0:06:54.95,0:06:58.85,NORMAL,,0000,0000,0000,,Ta butelka jest kością niezgody.
Dialogue: 0,0:07:03.49,0:07:07.40,NORMAL,,0000,0000,0000,,Jak mogłeś dać się nabrać\Nna taką tanią sztuczkę, kretynie?!
Dialogue: 0,0:07:07.40,0:07:09.80,NORMAL,,0000,0000,0000,,To by się nie stało,\Ngdybyś ze mną nie walczył!
Dialogue: 0,0:07:10.10,0:07:14.47,NORMAL,,0000,0000,0000,,Teraz wystarczy sobie tylko zażyczyć,\Nby Sasuke mnie pokochał.
Dialogue: 0,0:07:27.58,0:07:33.04,NORMAL,,0000,0000,0000,,Poproszę o butelkę,\Npani wielkie czoło!
Dialogue: 0,0:07:33.04,0:07:35.78,NORMAL,,0000,0000,0000,,Ino, ty świnio,\Npo co ci ona?!
Dialogue: 0,0:07:35.78,0:07:38.20,NORMAL,,0000,0000,0000,,Mam takie samo życzenie jak ty.
Dialogue: 0,0:07:38.20,0:07:39.63,NORMAL,,0000,0000,0000,,Coś ty powiedziała?!
Dialogue: 0,0:07:39.63,0:07:41.78,NORMAL,,0000,0000,0000,,Pewnie masz na myśli\Njakieś erotyczne igraszki!
Dialogue: 0,0:07:41.78,0:07:45.05,NORMAL,,0000,0000,0000,,Dlaczego tak myślisz?
Dialogue: 0,0:07:51.66,0:07:52.60,NORMAL,,0000,0000,0000,,Shino!
Dialogue: 0,0:07:54.01,0:07:57.14,NORMAL,,0000,0000,0000,,Potrzebuję tej butelki.
Dialogue: 0,0:07:57.80,0:08:00.25,NORMAL,,0000,0000,0000,,Oddawaj ją!
Dialogue: 0,0:08:00.25,0:08:06.15,NORMAL,,0000,0000,0000,,Zdobyłem ją, bo byłyście tak zajęte kłótnią,\Nże nie zauważyłyście moich owadów.
Dialogue: 0,0:08:06.86,0:08:08.54,NORMAL,,0000,0000,0000,,Wykorzystam zaistniałą sytuację.
Dialogue: 0,0:08:09.56,0:08:15.54,NORMAL,,0000,0000,0000,,Nadszedł czas, byście się dowiedziały\No mnie kilku rzeczy, więc posłuchajcie.
Dialogue: 0,0:08:15.54,0:08:17.38,NORMAL,,0000,0000,0000,,Moim ulubionym kolorem jest zielony.
Dialogue: 0,0:08:17.38,0:08:19.29,NORMAL,,0000,0000,0000,,Moim ulubionym\Nkwiatem kameleon.
Dialogue: 0,0:08:25.51,0:08:26.53,NORMAL,,0000,0000,0000,,Naruto!
Dialogue: 0,0:08:29.42,0:08:32.13,NORMAL,,0000,0000,0000,,Zamierzam zostać hokage!
Dialogue: 0,0:08:34.23,0:08:37.34,NORMAL,,0000,0000,0000,,Naruto, muszę poprosić cię\No oddanie butelki.
Dialogue: 0,0:08:37.34,0:08:38.53,NORMAL,,0000,0000,0000,,Brewka!
Dialogue: 0,0:08:38.53,0:08:40.48,NORMAL,,0000,0000,0000,,Jakie jest twoje życzenie?
Dialogue: 0,0:08:40.48,0:08:43.76,NORMAL,,0000,0000,0000,,Potężna dawka determinacji\Ni siły młodości!
Dialogue: 0,0:08:43.76,0:08:47.08,NORMAL,,0000,0000,0000,,Chcesz zabijać wzrokiem?
Dialogue: 0,0:08:47.40,0:08:48.98,NORMAL,,0000,0000,0000,,KLONY CIENIA!
Dialogue: 0,0:08:51.76,0:08:53.01,NORMAL,,0000,0000,0000,,Tenten, Lee!
Dialogue: 0,0:09:20.29,0:09:23.04,NORMAL,,0000,0000,0000,,Dlaczego oni ze sobą współpracują?
Dialogue: 0,0:09:23.04,0:09:27.51,NORMAL,,0000,0000,0000,,Zdobędą butelkę drużynowo,\Nby później zawalczyć o nią między sobą.
Dialogue: 0,0:09:27.51,0:09:29.78,NORMAL,,0000,0000,0000,,Rozumiem,\Nmądra taktyka.
Dialogue: 0,0:09:29.78,0:09:31.80,NORMAL,,0000,0000,0000,,Sasuke, też tak zróbmy!
Dialogue: 0,0:09:32.95,0:09:35.47,NORMAL,,0000,0000,0000,,Możesz o nim zapomnieć!
Dialogue: 0,0:09:35.90,0:09:38.25,NORMAL,,0000,0000,0000,,Sasuke, czekaj!
Dialogue: 0,0:09:40.19,0:09:42.14,NORMAL,,0000,0000,0000,,Gdzie reszta mojej drużyny?
Dialogue: 0,0:09:49.11,0:09:51.92,NORMAL,,0000,0000,0000,,Oddaj nam butelkę.
Dialogue: 0,0:09:53.35,0:09:54.33,NORMAL,,0000,0000,0000,,Naruto!
Dialogue: 0,0:09:55.24,0:09:57.10,NORMAL,,0000,0000,0000,,Naruto, rzuć mi ją!
Dialogue: 0,0:09:57.10,0:10:00.30,NORMAL,,0000,0000,0000,,Później rozstrzygniemy sprawę\Nbutelki w naszej drużynie.
Dialogue: 0,0:10:00.30,0:10:03.03,NORMAL,,0000,0000,0000,,Oszaleliście?!\NNie wierzę wam!
Dialogue: 0,0:10:03.03,0:10:05.93,NORMAL,,0000,0000,0000,,Chyba nie chcesz oddać\Nbutelki innej drużynie?
Dialogue: 0,0:10:05.93,0:10:08.19,NORMAL,,0000,0000,0000,,Zaufaj nam!
Dialogue: 0,0:10:09.58,0:10:11.05,NORMAL,,0000,0000,0000,,Interesujące.
Dialogue: 0,0:10:11.05,0:10:15.42,NORMAL,,0000,0000,0000,,Naprawdę uważasz,\Nże nie zabiorą butelki dla siebie?
Dialogue: 0,0:10:15.79,0:10:19.28,NORMAL,,0000,0000,0000,,Jeśli im ją oddasz, stracisz szansę\Nna zrealizowanie marzenia!
Dialogue: 0,0:10:20.70,0:10:24.09,NORMAL,,0000,0000,0000,,Oddaj ją nam!
Dialogue: 0,0:10:25.50,0:10:27.96,NORMAL,,0000,0000,0000,,Naruto, zaufaj nam!
Dialogue: 0,0:10:28.94,0:10:30.71,NORMAL,,0000,0000,0000,,Nie zawiedźcie mnie!
Dialogue: 0,0:10:37.90,0:10:39.54,NORMAL,,0000,0000,0000,,Dobra robota, Akamaru!
Dialogue: 0,0:10:48.29,0:10:50.57,NORMAL,,0000,0000,0000,,Oddawaj moją butelkę!
Dialogue: 0,0:10:51.15,0:10:53.90,NORMAL,,0000,0000,0000,,Akamaru, myśl już nad jakimś\Nfajnym życzeniem!
Dialogue: 0,0:11:02.58,0:11:03.87,NORMAL,,0000,0000,0000,,Dziękuję!
Dialogue: 0,0:11:04.20,0:11:06.10,NORMAL,,0000,0000,0000,,Spróbuj tego!
Dialogue: 0,0:11:06.10,0:11:08.45,NORMAL,,0000,0000,0000,,Nie pozwolę ci!
Dialogue: 0,0:11:12.33,0:11:13.88,NORMAL,,0000,0000,0000,,Gdzie się podziała butelka?!
Dialogue: 0,0:11:16.81,0:11:17.90,NORMAL,,0000,0000,0000,,Tam jest!
Dialogue: 0,0:11:29.62,0:11:31.32,NORMAL,,0000,0000,0000,,Dobra robota, Hinata!
Dialogue: 0,0:11:32.25,0:11:33.79,NORMAL,,0000,0000,0000,,Oddaj mi ją!
Dialogue: 0,0:11:34.89,0:11:36.53,NORMAL,,0000,0000,0000,,O czym ty gadasz?
Dialogue: 0,0:11:36.53,0:11:38.92,NORMAL,,0000,0000,0000,,Powinnaś ją oddać\Nswojej drużynie!
Dialogue: 0,0:11:42.50,0:11:44.82,NORMAL,,0000,0000,0000,,Oddaj mi butelkę!
Dialogue: 0,0:11:47.15,0:11:49.55,NORMAL,,0000,0000,0000,,Hinata, podaj do mnie!
Dialogue: 0,0:11:49.55,0:11:51.34,NORMAL,,0000,0000,0000,,Zamknąłbyś się w końcu!
Dialogue: 0,0:11:51.34,0:11:53.55,NORMAL,,0000,0000,0000,,Pewnie zażyczyłbyś sobie\Nczegoś głupiego!
Dialogue: 0,0:11:53.55,0:11:54.85,NORMAL,,0000,0000,0000,,Coś ty powiedziała?!
Dialogue: 0,0:11:54.85,0:11:56.83,NORMAL,,0000,0000,0000,,Ja chcę zostać hokage!
Dialogue: 0,0:11:56.83,0:12:00.23,NORMAL,,0000,0000,0000,,Hinata, na co czekasz?!\NOddaj mi butelkę.
Dialogue: 0,0:12:03.17,0:12:06.00,NORMAL,,0000,0000,0000,,Hinata, chodź do mamusi!
Dialogue: 0,0:12:06.00,0:12:09.14,NORMAL,,0000,0000,0000,,Hinata, oddaj mi butelkę!
Dialogue: 0,0:12:09.14,0:12:10.81,NORMAL,,0000,0000,0000,,Szybko, Hinata!
Dialogue: 0,0:12:10.81,0:12:16.82,NORMAL,,0000,0000,0000,,Przez tę butelkę\Nwalczycie między sobą.
Dialogue: 0,0:12:18.05,0:12:20.30,NORMAL,,0000,0000,0000,,Muszę ją zniszczyć!
Dialogue: 0,0:12:21.54,0:12:23.21,NORMAL,,0000,0000,0000,,Niedobrze,\Nnaprawdę chce to zrobić?!
Dialogue: 0,0:12:23.21,0:12:25.94,NORMAL,,0000,0000,0000,,Ona jedyna nie została\Nprzez nią zaślepiona!
Dialogue: 0,0:12:25.94,0:12:27.43,NORMAL,,0000,0000,0000,,Przepraszam was.
Dialogue: 0,0:12:30.83,0:12:31.75,NORMAL,,0000,0000,0000,,Moja ciało!
Dialogue: 0,0:12:33.25,0:12:36.71,NORMAL,,0000,0000,0000,,WIĄZANIE CIENIEM\NZAKOŃCZONE POWODZENIEM!
Dialogue: 0,0:12:36.71,0:12:38.44,NORMAL,,0000,0000,0000,,Shikamaru!
Dialogue: 0,0:12:38.44,0:12:40.24,NORMAL,,0000,0000,0000,,Posłuchajcie mnie!
Dialogue: 0,0:12:40.24,0:12:41.50,NORMAL,,0000,0000,0000,,Przymknij żeż się!
Dialogue: 0,0:12:41.50,0:12:44.12,NORMAL,,0000,0000,0000,,Nie oddam ci butelki!
Dialogue: 0,0:12:44.12,0:12:45.90,NORMAL,,0000,0000,0000,,Uspokój się.
Dialogue: 0,0:12:45.90,0:12:49.48,NORMAL,,0000,0000,0000,,Może istnieje sposób,\Nby spełnić wszystkie nasze prośby.
Dialogue: 0,0:12:50.19,0:12:51.02,NORMAL,,0000,0000,0000,,Niby jaki?
Dialogue: 0,0:12:52.10,0:12:55.26,NORMAL,,0000,0000,0000,,Zażyczymy sobie,\Nby spełniły się nasze życzenia.
Dialogue: 0,0:12:56.37,0:12:57.83,NORMAL,,0000,0000,0000,,Moje też?
Dialogue: 0,0:12:57.83,0:13:00.17,NORMAL,,0000,0000,0000,,Pokazałeś teraz swoje\Ndwie setki IQ!
Dialogue: 0,0:13:00.17,0:13:02.01,NORMAL,,0000,0000,0000,,Otwieram.
Dialogue: 0,0:13:09.82,0:13:11.98,NORMAL,,0000,0000,0000,,Zaczynało mi sie nudzić.
Dialogue: 0,0:13:12.71,0:13:15.11,NORMAL,,0000,0000,0000,,Jakie jest wasze ostatnie życzenie?
Dialogue: 0,0:13:15.11,0:13:17.28,NORMAL,,0000,0000,0000,,Już wiemy,\Nczego chcemy!
Dialogue: 0,0:13:17.28,0:13:19.46,NORMAL,,0000,0000,0000,,Chętnie posłucham!
Dialogue: 0,0:13:19.46,0:13:21.92,NORMAL,,0000,0000,0000,,Ostatnim życzeń jest...
Dialogue: 0,0:13:21.92,0:13:24.93,NORMAL,,0000,0000,0000,,...dajcie mi parę groszy!
Dialogue: 0,0:13:24.93,0:13:27.95,NORMAL,,0000,0000,0000,,Automaty nie zwracają reszty.
Dialogue: 0,0:13:27.95,0:13:31.79,NORMAL,,0000,0000,0000,,Twoje życzenie jest dla mnie rozkazem!
Dialogue: 0,0:13:35.48,0:13:37.27,NORMAL,,0000,0000,0000,,Dzięki.
Dialogue: 0,0:13:37.67,0:13:39.41,NORMAL,,0000,0000,0000,,A korzystaj do woli.
Dialogue: 0,0:13:51.56,0:13:54.40,NORMAL,,0000,0000,0000,,Do zobaczenia za tysiąc lat!
Dialogue: 0,0:13:57.89,0:13:58.97,NORMAL,,0000,0000,0000,,Co to takiego?
Dialogue: 0,0:14:02.60,0:14:04.05,NORMAL,,0000,0000,0000,,Co się wam stało?
Dialogue: 0,0:14:05.60,0:14:08.50,NORMAL,,0000,0000,0000,,Ciąg dalszy NIE nastąpi
