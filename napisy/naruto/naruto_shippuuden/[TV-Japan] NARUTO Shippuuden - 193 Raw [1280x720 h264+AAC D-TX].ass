Dialogue: 0,0:00:09.53,0:00:10.93,NORMAL,,0000,0000,0000,,Shikamaru, masz chwilkę?
Dialogue: 0,0:00:11.93,0:00:13.07,NORMAL,,0000,0000,0000,,O co chodzi?
Dialogue: 0,0:00:13.10,0:00:14.70,NORMAL,,0000,0000,0000,,Ino cię szuka.
Dialogue: 0,0:00:15.57,0:00:16.73,NORMAL,,0000,0000,0000,,Co robicie?
Dialogue: 0,0:00:17.20,0:00:18.90,NORMAL,,0000,0000,0000,,Segregujemy zdjęcia.
Dialogue: 0,0:00:18.97,0:00:20.73,NORMAL,,0000,0000,0000,,Po co?
Dialogue: 0,0:00:21.17,0:00:23.63,NORMAL,,0000,0000,0000,,Te zdjęcia przetrwały\Nostatni atak na wioskę.
Dialogue: 0,0:00:24.47,0:00:29.10,NORMAL,,0000,0000,0000,,Na pewno są cenne dla właścicieli,\Ndlatego chcieliśmy je zwrócić.
Dialogue: 0,0:00:29.63,0:00:32.27,NORMAL,,0000,0000,0000,,Przejrzyj je i powiedz,\Nczy kogoś rozpoznajesz.
Dialogue: 0,0:00:32.70,0:00:35.93,NORMAL,,0000,0000,0000,,Jasne, chętnie wam w tym pomogę.
Dialogue: 0,0:00:37.47,0:00:39.97,NORMAL,,0000,0000,0000,,Nikogo z nich nie znam.
Dialogue: 0,0:00:41.87,0:00:42.80,NORMAL,,0000,0000,0000,,Kojarzę go!
Dialogue: 0,0:00:43.17,0:00:45.23,NORMAL,,0000,0000,0000,,Mam coś do zrobienia!
Dialogue: 0,0:00:46.30,0:00:49.00,NORMAL,,0000,0000,0000,,- Zabieram to zdjęcie.\N- Naruto!
Dialogue: 0,0:00:49.43,0:00:51.23,NORMAL,,0000,0000,0000,,Zapomniałbym.
Dialogue: 0,0:00:51.23,0:00:53.47,NORMAL,,0000,0000,0000,,Widzieliście gdzieś w pobliżu\Nkwitnące tulipany?
Dialogue: 0,0:00:53.70,0:00:55.10,NORMAL,,0000,0000,0000,,Tulipany?
Dialogue: 0,0:00:55.23,0:00:58.13,NORMAL,,0000,0000,0000,,Te kwiaty są takie romantyczne!
Dialogue: 0,0:00:58.70,0:01:02.47,NORMAL,,0000,0000,0000,,Symbolizują wieczną miłość.
Dialogue: 0,0:01:03.20,0:01:05.67,NORMAL,,0000,0000,0000,,To jej spojrzenie mnie przeraża.
Dialogue: 0,0:01:07.13,0:01:10.87,NORMAL,,0000,0000,0000,,Ogród we wschodniej części wioski\Nchyba nie został zniszczony podczas ataku.
Dialogue: 0,0:01:11.17,0:01:12.50,NORMAL,,0000,0000,0000,,Dzięki, Shikamaru!
Dialogue: 0,0:01:13.03,0:01:13.93,NORMAL,,0000,0000,0000,,Nie ma za co.
Dialogue: 0,0:01:14.43,0:01:17.23,NORMAL,,0000,0000,0000,,Po co mu tulipany?
Dialogue: 0,0:01:17.57,0:01:18.70,NORMAL,,0000,0000,0000,,Nie mam pojęcia.
Dialogue: 0,0:01:21.00,0:01:22.67,NORMAL,,0000,0000,0000,,Wieczna miłość?
Dialogue: 0,0:01:29.03,0:01:30.93,NORMAL,,0000,0000,0000,,Zaczyna padać.
Dialogue: 0,0:01:31.03,0:01:32.70,NORMAL,,0000,0000,0000,,Muszę wracać do wioski.
Dialogue: 0,0:01:51.47,0:01:52.97,NORMAL,,0000,0000,0000,,Co to jest?
Dialogue: 0,0:01:56.77,0:01:58.63,NORMAL,,0000,0000,0000,,To tylko skała.
Dialogue: 0,0:02:01.47,0:02:02.43,NORMAL,,0000,0000,0000,,A to co?
Dialogue: 0,0:02:03.23,0:02:04.30,NORMAL,,0000,0000,0000,,Kartka?
Dialogue: 0,0:02:04.67,0:02:06.00,NORMAL,,0000,0000,0000,,Kisuke?
Dialogue: 0,0:02:06.80,0:02:08.20,NORMAL,,0000,0000,0000,,Reszta jest nieczytelna.
Dialogue: 0,0:02:09.00,0:02:11.03,NORMAL,,0000,0000,0000,,Cholera!
Dialogue: 0,0:02:14.57,0:02:16.33,NORMAL,,0000,0000,0000,,Lepiej już sobie stąd pójdę.
Dialogue: 0,0:02:32.37,0:02:34.03,NORMAL,,0000,0000,0000,,Sakurcia!
Dialogue: 0,0:02:35.93,0:02:39.83,NORMAL,,0000,0000,0000,,Sakura, możesz być delikatniejsza?
Dialogue: 0,0:02:41.47,0:02:43.57,NORMAL,,0000,0000,0000,,Sakura, ciałka ci przybyło?
Dialogue: 0,0:02:47.37,0:02:50.13,NORMAL,,0000,0000,0000,,Kim pan jest?!\NWystraszyłem się.
Dialogue: 0,0:02:50.17,0:02:53.40,NORMAL,,0000,0000,0000,,To ja tu jestem przerażony!\NWrzeszczałeś jak opętany.
Dialogue: 0,0:02:53.47,0:02:57.00,NORMAL,,0000,0000,0000,,Nie wolno wchodzić\Ndo czyjegoś domu bez pozwolenia!
Dialogue: 0,0:03:00.07,0:03:03.00,NORMAL,,0000,0000,0000,,Gdy się ocknąłem,\Njuż tutaj byłem.
Dialogue: 0,0:03:04.53,0:03:07.63,NORMAL,,0000,0000,0000,,Gdy się pan ocknął?
Dialogue: 0,0:03:07.73,0:03:10.13,NORMAL,,0000,0000,0000,,Jak to możliwe?
Dialogue: 0,0:03:11.10,0:03:14.07,NORMAL,,0000,0000,0000,,- Duch!\N- Co, jak, gdzie?
Dialogue: 0,0:03:16.60,0:03:18.37,NORMAL,,0000,0000,0000,,Nie ma tu żadnego ducha!
Dialogue: 0,0:03:18.43,0:03:19.83,NORMAL,,0000,0000,0000,,Chcesz mnie przyprawić\No atak serca?!
Dialogue: 0,0:03:20.30,0:03:23.10,NORMAL,,0000,0000,0000,,Popatrz pan w lustro!
Dialogue: 0,0:03:28.93,0:03:31.20,NORMAL,,0000,0000,0000,,Nie łam się pan.
Dialogue: 0,0:03:34.20,0:03:35.73,NORMAL,,0000,0000,0000,,Chyba proszę o zbyt wiele.
Dialogue: 0,0:03:38.23,0:03:41.27,NORMAL,,0000,0000,0000,,Nie pamięta pan swojej śmierci?
Dialogue: 0,0:03:43.03,0:03:44.00,NORMAL,,0000,0000,0000,,No tak!
Dialogue: 0,0:03:46.33,0:03:47.43,NORMAL,,0000,0000,0000,,Co się stało?
Dialogue: 0,0:03:48.67,0:03:50.07,NORMAL,,0000,0000,0000,,Niczego nie pamiętam.
Dialogue: 0,0:03:52.63,0:03:55.37,NORMAL,,0000,0000,0000,,Nawet nie pamiętam swojego imienia.
Dialogue: 0,0:03:55.40,0:03:58.70,NORMAL,,0000,0000,0000,,Nigdy nie słyszałem o duchu z amnezją!
Dialogue: 0,0:03:59.10,0:04:00.23,NORMAL,,0000,0000,0000,,Ja też nie.
Dialogue: 0,0:04:00.43,0:04:03.13,NORMAL,,0000,0000,0000,,Czy zrobiłeś coś, co mogłoby ściągnąć\Nna ciebie mój gniew?
Dialogue: 0,0:04:03.20,0:04:05.50,NORMAL,,0000,0000,0000,,Oczywiście, że nie!
Dialogue: 0,0:04:05.67,0:04:08.00,NORMAL,,0000,0000,0000,,Nawet pana nie znam!
Dialogue: 0,0:04:08.00,0:04:10.23,NORMAL,,0000,0000,0000,,Jakim cudem miałbym\Ncoś panu zrobić?!
Dialogue: 0,0:04:10.57,0:04:12.27,NORMAL,,0000,0000,0000,,- Moment!\N- Przypomniałeś sobie?
Dialogue: 0,0:04:12.30,0:04:17.03,NORMAL,,0000,0000,0000,,Ostatniej nocy zerwałem\Ndziwną kartkę.
Dialogue: 0,0:04:18.23,0:04:19.23,NORMAL,,0000,0000,0000,,Kartkę?
Dialogue: 0,0:04:19.30,0:04:20.33,NORMAL,,0000,0000,0000,,Momencik!
Dialogue: 0,0:04:21.57,0:04:23.60,NORMAL,,0000,0000,0000,,I niby to mnie\Nzmieniło w ducha?
Dialogue: 0,0:04:24.63,0:04:25.77,NORMAL,,0000,0000,0000,,To ta kartka!
Dialogue: 0,0:04:26.63,0:04:29.57,NORMAL,,0000,0000,0000,,Więc nazywam się Kisuke?
Dialogue: 0,0:04:30.53,0:04:35.27,NORMAL,,0000,0000,0000,,Przyjmijmy, że nazywam się Kisuke,\Nale skąd pochodzę?
Dialogue: 0,0:04:35.37,0:04:37.07,NORMAL,,0000,0000,0000,,Skąd mam wiedzieć?
Dialogue: 0,0:04:37.13,0:04:38.37,NORMAL,,0000,0000,0000,,Racja.
Dialogue: 0,0:04:38.83,0:04:42.43,NORMAL,,0000,0000,0000,,Skoro jestem martwy, dlaczego\Nnie odszedłem na tamten świat?
Dialogue: 0,0:04:43.43,0:04:48.03,NORMAL,,0000,0000,0000,,Słyszałem, że duchy błąkają się po Ziemi,\Ngdy trzyma ich tutaj jakaś silna więź.
Dialogue: 0,0:04:48.07,0:04:50.30,NORMAL,,0000,0000,0000,,Silna więź?
Dialogue: 0,0:04:51.20,0:04:52.47,NORMAL,,0000,0000,0000,,Co to może być?
Dialogue: 0,0:04:54.13,0:04:56.47,NORMAL,,0000,0000,0000,,I co ja z nim teraz pocznę?
Dialogue: 0,0:05:03.63,0:05:10.06,NORMAL,,0000,0000,0000,,Nieboskłon zaczyna odpływać w dal,
Dialogue: 0,0:05:10.06,0:05:16.40,NORMAL,,0000,0000,0000,,jego czysty błękit już dawno przestał\Nwskazywać na lepsze jutro.
Dialogue: 0,0:05:16.40,0:05:22.84,NORMAL,,0000,0000,0000,,Już nawet nie pamiętam,\Nkiedy zacząłem tonąć w tych mętnych głębinach...
Dialogue: 0,0:05:22.84,0:05:29.40,NORMAL,,0000,0000,0000,,...zamieszkanych przez ludzi niepotrafiących\Nczerpać powietrza pełną piersią.
Dialogue: 0,0:05:30.00,0:05:36.00,NORMAL,,0000,0000,0000,,Mógłbym pozbyć się tego całego smutku\Ni przestać oglądać się za siebie,
Dialogue: 0,0:05:36.00,0:05:41.56,NORMAL,,0000,0000,0000,,jednak wtedy nie byłbym już tą samą osobą.
Dialogue: 0,0:05:41.96,0:05:49.44,NORMAL,,0000,0000,0000,,Nawet jeśli cały świat wystąpi przeciwko mnie,\Nnadal będzie istniał cień nadziei.
Dialogue: 0,0:05:49.44,0:05:55.00,NORMAL,,0000,0000,0000,,Skoro mogę posunąć się tak daleko,
Dialogue: 0,0:05:55.00,0:06:01.44,NORMAL,,0000,0000,0000,,pragnę zaczerpnąć powietrza!\NTo miejsce mnie przytłacza!
Dialogue: 0,0:06:01.44,0:06:07.84,NORMAL,,0000,0000,0000,,Całe życie widziałem tylko mrok,\Nbyłem jak tonący bez deski ratunku,
Dialogue: 0,0:06:07.84,0:06:14.24,NORMAL,,0000,0000,0000,,dlatego teraz chcę wypłynąć\Ni zacząć oddychać!
Dialogue: 0,0:06:14.24,0:06:22.34,NORMAL,,0000,0000,0000,,Jeśli doświadczę chociaż odrobiny szczęścia,\Njuż nigdy nie pochłonie mnie ta mroczna otchłań.
Dialogue: 0,0:06:27.27,0:06:31.97,NORMAL,,0000,0000,0000,,Z KRONIK WIOSKI LIŚCIA:\NCZŁOWIEK, KTÓRY UMARŁ DWA RAZY.
Dialogue: 0,0:06:36.03,0:06:39.33,NORMAL,,0000,0000,0000,,Wygląda na to,\Nże tylko ja pana widzę.
Dialogue: 0,0:06:39.43,0:06:41.87,NORMAL,,0000,0000,0000,,Więc może mnie widzieć tylko osoba,\Nktóra zerwała pieczęć.
Dialogue: 0,0:06:41.90,0:06:44.87,NORMAL,,0000,0000,0000,,Tylko ty mi możesz pomóc.
Dialogue: 0,0:06:45.77,0:06:48.53,NORMAL,,0000,0000,0000,,Pokręcę się po okolicy\Ni popytam o ciebie.
Dialogue: 0,0:06:49.13,0:06:51.13,NORMAL,,0000,0000,0000,,Kisuke?\NNie znam.
Dialogue: 0,0:06:51.77,0:06:53.43,NORMAL,,0000,0000,0000,,Pierwsze słyszę.
Dialogue: 0,0:06:54.50,0:06:55.83,NORMAL,,0000,0000,0000,,Nie znam tego pana.
Dialogue: 0,0:07:03.17,0:07:06.33,NORMAL,,0000,0000,0000,,Nikt pana nie zna na dzielnicy.
Dialogue: 0,0:07:06.53,0:07:07.77,NORMAL,,0000,0000,0000,,Więc to na nic?
Dialogue: 0,0:07:07.80,0:07:09.20,NORMAL,,0000,0000,0000,,Już wiem!
Dialogue: 0,0:07:14.70,0:07:17.33,NORMAL,,0000,0000,0000,,Trzeci jest teraz zajęty\Ni nikogo nie przyjmuje.
Dialogue: 0,0:07:17.63,0:07:19.03,NORMAL,,0000,0000,0000,,Czy coś się stało?
Dialogue: 0,0:07:19.43,0:07:21.50,NORMAL,,0000,0000,0000,,Bierze udział w ważnej naradzie.
Dialogue: 0,0:07:23.70,0:07:27.60,NORMAL,,0000,0000,0000,,Staruszek Hokage\Npewnie by coś wiedział.
Dialogue: 0,0:07:28.13,0:07:30.70,NORMAL,,0000,0000,0000,,Po prostu spytamy kogoś innego.
Dialogue: 0,0:07:36.87,0:07:43.73,NORMAL,,0000,0000,0000,,Nowy rozkład jednostek pozwoli nam\Npotroić siłę systemu obronnego naszej wioski.
Dialogue: 0,0:07:44.00,0:07:50.50,NORMAL,,0000,0000,0000,,Uważam ten krok za absolutnie konieczny\Ni upraszam o zaakceptowanie zmian.
Dialogue: 0,0:07:51.77,0:07:56.30,NORMAL,,0000,0000,0000,,Rozumiem,\Ncałość prezentuje się solidnie.
Dialogue: 0,0:08:00.63,0:08:04.83,NORMAL,,0000,0000,0000,,Podejmiemy decyzję dzisiaj po południu.\NTeraz zrobimy sobie przerwę.
Dialogue: 0,0:08:09.17,0:08:10.93,NORMAL,,0000,0000,0000,,Pani Koharu, panie Homura!
Dialogue: 0,0:08:12.60,0:08:13.63,NORMAL,,0000,0000,0000,,To ty, Sabiru?
Dialogue: 0,0:08:14.17,0:08:17.07,NORMAL,,0000,0000,0000,,- Chciałby państwu podziękować.\N- To był ciekawy pomysł.
Dialogue: 0,0:08:17.60,0:08:21.90,NORMAL,,0000,0000,0000,,Zaszedłeś tak daleko\Ndzięki swojej ciężkiej pracy.
Dialogue: 0,0:08:22.20,0:08:23.63,NORMAL,,0000,0000,0000,,Dziękuję.
Dialogue: 0,0:08:24.43,0:08:27.17,NORMAL,,0000,0000,0000,,Chcę spędzić swoje życie,\Npracując dla wioski.
Dialogue: 0,0:08:29.37,0:08:33.47,NORMAL,,0000,0000,0000,,W tak młodym wieku wymyślił\Nnowy system obronny wioski.
Dialogue: 0,0:08:33.47,0:08:35.13,NORMAL,,0000,0000,0000,,Imponujące.
Dialogue: 0,0:08:36.63,0:08:43.73,NORMAL,,0000,0000,0000,,Jeśli zmiany zostaną wprowadzone na popołudniowej\Nsesji, mieszkańcy poczują się jeszcze bezpieczniejsi.
Dialogue: 0,0:08:45.07,0:08:46.90,NORMAL,,0000,0000,0000,,Nie mamy szczęścia.
Dialogue: 0,0:08:50.00,0:08:52.43,NORMAL,,0000,0000,0000,,Zaczynam tracić nadzieję\Nna odzyskanie wspomnień.
Dialogue: 0,0:08:53.27,0:08:56.47,NORMAL,,0000,0000,0000,,Głowa do góry,\Nnie można się poddawać.
Dialogue: 0,0:08:56.67,0:08:58.43,NORMAL,,0000,0000,0000,,Naruto!
Dialogue: 0,0:08:58.57,0:08:59.43,NORMAL,,0000,0000,0000,,Sakura!
Dialogue: 0,0:08:59.50,0:09:03.50,NORMAL,,0000,0000,0000,,Zamiast przyjść na trening,\Nszwendasz się po wiosce?
Dialogue: 0,0:09:03.73,0:09:05.07,NORMAL,,0000,0000,0000,,Zapomniałem o tym!
Dialogue: 0,0:09:05.23,0:09:06.93,NORMAL,,0000,0000,0000,,Wymyśliłbyś coś lepszego!
Dialogue: 0,0:09:08.27,0:09:11.50,NORMAL,,0000,0000,0000,,Mam poważnego kłopoty.
Dialogue: 0,0:09:11.53,0:09:12.67,NORMAL,,0000,0000,0000,,Kłopoty?
Dialogue: 0,0:09:12.70,0:09:16.77,NORMAL,,0000,0000,0000,,Popatrz na niego!\NNo tak, tylko ja go widzę.
Dialogue: 0,0:09:16.90,0:09:19.20,NORMAL,,0000,0000,0000,,Gadasz od rzeczy!
Dialogue: 0,0:09:19.83,0:09:22.57,NORMAL,,0000,0000,0000,,Jeśli będziesz urywał się z treningów,\Ndoniosę na ciebie sensejowi!
Dialogue: 0,0:09:22.63,0:09:24.60,NORMAL,,0000,0000,0000,,Rozumiem.
Dialogue: 0,0:09:24.70,0:09:25.57,NORMAL,,0000,0000,0000,,Masz ci los.
Dialogue: 0,0:09:28.10,0:09:30.43,NORMAL,,0000,0000,0000,,Niech to, w co ja się wplątałem?
Dialogue: 0,0:09:30.87,0:09:32.37,NORMAL,,0000,0000,0000,,Przepraszam, Naruto.
Dialogue: 0,0:09:32.50,0:09:34.37,NORMAL,,0000,0000,0000,,Zaczekaj, Sakura!
Dialogue: 0,0:09:34.97,0:09:35.83,NORMAL,,0000,0000,0000,,Czego?!
Dialogue: 0,0:09:35.83,0:09:38.70,NORMAL,,0000,0000,0000,,Znasz może osobę\Nnazywającą się Kisuke?
Dialogue: 0,0:09:39.33,0:09:40.10,NORMAL,,0000,0000,0000,,Kisuke?
Dialogue: 0,0:09:40.83,0:09:43.10,NORMAL,,0000,0000,0000,,Nie znam nikogo takiego.
Dialogue: 0,0:09:45.10,0:09:47.17,NORMAL,,0000,0000,0000,,Już gdzieś słyszałam to imię.
Dialogue: 0,0:09:47.20,0:09:48.00,NORMAL,,0000,0000,0000,,Poważnie?!
Dialogue: 0,0:09:48.03,0:09:49.73,NORMAL,,0000,0000,0000,,Gdzie?!
Dialogue: 0,0:09:52.20,0:09:53.20,NORMAL,,0000,0000,0000,,Już wiem.
Dialogue: 0,0:09:53.27,0:09:54.50,NORMAL,,0000,0000,0000,,SZPITAL WIOSKI LIŚCIA
Dialogue: 0,0:09:58.60,0:10:03.07,NORMAL,,0000,0000,0000,,Gdy poszłam do szpitala w odwiedziny, zauważyłam\Nto imię obok ostatnich drzwi w korytarzu.
Dialogue: 0,0:10:04.97,0:10:06.57,NORMAL,,0000,0000,0000,,KISUKE MABOROSHI
Dialogue: 0,0:10:19.93,0:10:21.27,NORMAL,,0000,0000,0000,,Nie wierzę!
Dialogue: 0,0:10:22.23,0:10:23.23,NORMAL,,0000,0000,0000,,To przecież ja!
Dialogue: 0,0:10:24.83,0:10:28.40,NORMAL,,0000,0000,0000,,Nic dziwnego,\Nże nie może pan spoczywać w spokoju.
Dialogue: 0,0:10:29.27,0:10:31.80,NORMAL,,0000,0000,0000,,Pan nadal żyje.
Dialogue: 0,0:10:32.80,0:10:36.13,NORMAL,,0000,0000,0000,,Co mi się stało?
Dialogue: 0,0:10:48.17,0:10:50.33,NORMAL,,0000,0000,0000,,Proszę pana!
Dialogue: 0,0:10:52.80,0:10:53.80,NORMAL,,0000,0000,0000,,Naruto?
Dialogue: 0,0:10:54.10,0:10:57.17,NORMAL,,0000,0000,0000,,Zdobyłem pana adres,\Nchodźmy tam.
Dialogue: 0,0:10:59.03,0:11:00.00,NORMAL,,0000,0000,0000,,Proszę pana!
Dialogue: 0,0:11:00.30,0:11:01.87,NORMAL,,0000,0000,0000,,Dobrze.
Dialogue: 0,0:11:12.67,0:11:14.63,NORMAL,,0000,0000,0000,,To mój dom?
Dialogue: 0,0:11:26.67,0:11:27.67,NORMAL,,0000,0000,0000,,Tulipan?
Dialogue: 0,0:11:30.67,0:11:34.20,NORMAL,,0000,0000,0000,,Pański dom spalił się\Njakiś czas temu.
Dialogue: 0,0:11:34.87,0:11:39.77,NORMAL,,0000,0000,0000,,Przykro mi to mówić,\Nale pańska żona zginęła w pożarze.
Dialogue: 0,0:11:42.00,0:11:42.80,NORMAL,,0000,0000,0000,,Proszę pana?
Dialogue: 0,0:11:53.70,0:11:55.03,NORMAL,,0000,0000,0000,,Wszystko w porządku?
Dialogue: 0,0:12:00.17,0:12:00.93,NORMAL,,0000,0000,0000,,Przypomniałem sobie.
Dialogue: 0,0:12:02.73,0:12:04.83,NORMAL,,0000,0000,0000,,Zostałem zapieczętowany...
Dialogue: 0,0:12:06.77,0:12:08.13,NORMAL,,0000,0000,0000,,...przez tamtego człowieka.
Dialogue: 0,0:12:08.57,0:12:09.33,NORMAL,,0000,0000,0000,,Kto to był?
Dialogue: 0,0:12:14.63,0:12:19.57,NORMAL,,0000,0000,0000,,Dostałem rozkaz od Oddziałów Anbu,\Nby w naszej wiosce odszukać szpiega.
Dialogue: 0,0:12:21.03,0:12:24.73,NORMAL,,0000,0000,0000,,Jednym z podejrzanych był Sabiru.
Dialogue: 0,0:12:25.37,0:12:28.27,NORMAL,,0000,0000,0000,,To wyjątkowo uzdolniony ninja\Nz Oddziału Taktycznego Ochraniającego Wioskę.
Dialogue: 0,0:12:28.27,0:12:32.23,NORMAL,,0000,0000,0000,,Wszyscy uważają, że w przyszłości\Nto on zapewni wiosce bezpieczeństwo.
Dialogue: 0,0:12:33.70,0:12:37.57,NORMAL,,0000,0000,0000,,Dlatego nie mogłem\Noskarżyć go bez dowodów.
Dialogue: 0,0:12:38.10,0:12:42.93,NORMAL,,0000,0000,0000,,Dlatego kontynuowałem dochodzenie,\Nnie mówiąc nikomu o moich podejrzeniach.
Dialogue: 0,0:12:44.17,0:12:45.37,NORMAL,,0000,0000,0000,,I wtedy...
Dialogue: 0,0:12:47.40,0:12:48.40,NORMAL,,0000,0000,0000,,Wyłaź!
Dialogue: 0,0:12:49.80,0:12:52.17,NORMAL,,0000,0000,0000,,Myślisz, że cię nie zauważyłem?
Dialogue: 0,0:12:54.17,0:12:58.53,NORMAL,,0000,0000,0000,,Zdaje się, że mnie rozszyfrowałeś.\NGra skończona.
Dialogue: 0,0:12:59.73,0:13:01.07,NORMAL,,0000,0000,0000,,Więc to naprawdę byłeś ty!
Dialogue: 0,0:13:03.07,0:13:04.17,NORMAL,,0000,0000,0000,,Nie uciekniesz mi!
Dialogue: 0,0:13:14.00,0:13:14.87,NORMAL,,0000,0000,0000,,Co jest?!
Dialogue: 0,0:13:15.80,0:13:16.77,NORMAL,,0000,0000,0000,,Co to?!
Dialogue: 0,0:13:17.73,0:13:18.87,NORMAL,,0000,0000,0000,,To koniec.
Dialogue: 0,0:13:19.03,0:13:20.13,NORMAL,,0000,0000,0000,,Cholera!
Dialogue: 0,0:13:20.77,0:13:21.93,NORMAL,,0000,0000,0000,,Żartowałem!
Dialogue: 0,0:13:37.73,0:13:39.20,NORMAL,,0000,0000,0000,,Myślałem,\Nże będziesz silniejszy.
Dialogue: 0,0:13:41.80,0:13:46.57,NORMAL,,0000,0000,0000,,Wybacz, że ci to mówię, ale przed chwilą\Npozbyłem się również twojej żony.
Dialogue: 0,0:13:47.03,0:13:48.03,NORMAL,,0000,0000,0000,,Coś ty powiedział?!
Dialogue: 0,0:13:51.63,0:13:53.10,NORMAL,,0000,0000,0000,,Skurwysynu!
Dialogue: 0,0:13:53.83,0:13:59.10,NORMAL,,0000,0000,0000,,Zapieczętuję tutaj twoją duszę,\Na ciału pozwolę odpłynąć.
Dialogue: 0,0:13:59.83,0:14:04.63,NORMAL,,0000,0000,0000,,{\fscx95}Wszyscy pomyślą, że nie wytrzymałeś stresu związanego\Nz otrzymanym zadaniem i rzuciłeś się z żoną do rzeki,
Dialogue: 0,0:14:05.20,0:14:07.03,NORMAL,,0000,0000,0000,,popełniając zbiorowe samobójstwo.
Dialogue: 0,0:14:07.90,0:14:08.90,NORMAL,,0000,0000,0000,,Żegnaj!
Dialogue: 0,0:14:09.27,0:14:10.80,NORMAL,,0000,0000,0000,,ODDZIELENIE DUSZY!
Dialogue: 0,0:14:33.23,0:14:38.13,NORMAL,,0000,0000,0000,,Sabiru chce wprowadzić swój\Nsystem obronny w naszej wiosce,
Dialogue: 0,0:14:38.73,0:14:42.77,NORMAL,,0000,0000,0000,,dzięki czemu będzie miał pełną kontrolę\Nnad naszymi zabezpieczeniami.
Dialogue: 0,0:14:44.27,0:14:46.60,NORMAL,,0000,0000,0000,,Trzeba go zatrzymać.
Dialogue: 0,0:14:46.73,0:14:48.20,NORMAL,,0000,0000,0000,,Ale jak?
Dialogue: 0,0:15:15.30,0:15:16.47,NORMAL,,0000,0000,0000,,Spada temperatura ciała!
Dialogue: 0,0:15:17.13,0:15:18.47,NORMAL,,0000,0000,0000,,Tętno dalej niewyczuwalne!
Dialogue: 0,0:15:19.40,0:15:20.37,NORMAL,,0000,0000,0000,,Nie możemy mu pomóc!
Dialogue: 0,0:15:23.03,0:15:25.17,NORMAL,,0000,0000,0000,,Co się dzieje?!
Dialogue: 0,0:15:25.60,0:15:27.23,NORMAL,,0000,0000,0000,,Wyjdź stąd, przeszkadzasz nam!
Dialogue: 0,0:15:27.37,0:15:29.47,NORMAL,,0000,0000,0000,,Co mu się stało?!
Dialogue: 0,0:15:29.50,0:15:32.43,NORMAL,,0000,0000,0000,,Aparatura podtrzymująca życie\Nprzestała działać.
Dialogue: 0,0:15:33.17,0:15:34.57,NORMAL,,0000,0000,0000,,Brak tętna!
Dialogue: 0,0:15:35.37,0:15:36.23,NORMAL,,0000,0000,0000,,To koniec.
Dialogue: 0,0:15:38.50,0:15:39.50,NORMAL,,0000,0000,0000,,Nie wierzę!
Dialogue: 0,0:15:40.63,0:15:44.30,NORMAL,,0000,0000,0000,,Nie może pan umrzeć!
Dialogue: 0,0:15:45.83,0:15:46.90,NORMAL,,0000,0000,0000,,Jak pan mógł?!
Dialogue: 0,0:15:49.63,0:15:51.13,NORMAL,,0000,0000,0000,,Nie czas na łzy, Naruto!
Dialogue: 0,0:15:53.67,0:15:54.53,NORMAL,,0000,0000,0000,,Proszę pana!
Dialogue: 0,0:15:54.87,0:15:57.47,NORMAL,,0000,0000,0000,,Ktoś zatrzymał aparaturę\Npodtrzymującą życie, co mnie zabiło.
Dialogue: 0,0:15:58.90,0:16:01.73,NORMAL,,0000,0000,0000,,Sprawcą jest osoba,\Ndla której byłem zagrożeniem.
Dialogue: 0,0:16:02.87,0:16:03.77,NORMAL,,0000,0000,0000,,Sabiru?
Dialogue: 0,0:16:04.70,0:16:10.50,NORMAL,,0000,0000,0000,,Teraz będziemy głosować, czy wprowadzimy\Nw Wiosce Liścia system obronny Sabiru.
Dialogue: 0,0:16:11.93,0:16:14.83,NORMAL,,0000,0000,0000,,Osoby, która są za,\Nproszę o podniesienie ręki!
Dialogue: 0,0:16:23.90,0:16:26.67,NORMAL,,0000,0000,0000,,Większością głosów przyjmujemy...
Dialogue: 0,0:16:26.70,0:16:27.57,NORMAL,,0000,0000,0000,,Sprzeciw!
Dialogue: 0,0:16:29.97,0:16:31.97,NORMAL,,0000,0000,0000,,Proszę wstrzymać się z decyzją!
Dialogue: 0,0:16:32.10,0:16:33.73,NORMAL,,0000,0000,0000,,- Kim pan jest?\N- Kisuke?
Dialogue: 0,0:16:33.93,0:16:36.17,NORMAL,,0000,0000,0000,,Jak uzasadnisz swój sprzeciw?
Dialogue: 0,0:16:36.80,0:16:39.43,NORMAL,,0000,0000,0000,,Nazywam się Kisuke\Ni pracuję dla Oddziału Anbu.
Dialogue: 0,0:16:39.87,0:16:43.90,NORMAL,,0000,0000,0000,,Dostałem rozkaz wykrycia szpiega\Ninfiltrującego naszą wioskę.
Dialogue: 0,0:16:44.83,0:16:48.40,NORMAL,,0000,0000,0000,,Jaki to ma związek\Nz dzisiejszym głosowaniem?
Dialogue: 0,0:16:48.50,0:16:50.07,NORMAL,,0000,0000,0000,,To ma ogromny związek!
Dialogue: 0,0:16:50.90,0:16:54.07,NORMAL,,0000,0000,0000,,Naruto, uspokój się,\Nbo wykryją twoją technikę transformacji.
Dialogue: 0,0:16:55.30,0:16:56.53,NORMAL,,0000,0000,0000,,Dobrze.
Dialogue: 0,0:16:59.00,0:17:01.90,NORMAL,,0000,0000,0000,,Te dwie sprawy\Nmają ze sobą wiele wspólnego.
Dialogue: 0,0:17:02.13,0:17:06.60,NORMAL,,0000,0000,0000,,Tym szpiegiem jest Sabiru!
Dialogue: 0,0:17:07.40,0:17:08.70,NORMAL,,0000,0000,0000,,- Co?\N- Niemożliwe!
Dialogue: 0,0:17:08.87,0:17:10.03,NORMAL,,0000,0000,0000,,Nie wyciągajcie\Npochopnych wniosków!
Dialogue: 0,0:17:11.27,0:17:13.83,NORMAL,,0000,0000,0000,,Kisuke, masz na to jakiś dowód?
Dialogue: 0,0:17:14.13,0:17:16.80,NORMAL,,0000,0000,0000,,Zapieczętowałeś moją duszę!
Dialogue: 0,0:17:16.83,0:17:18.67,NORMAL,,0000,0000,0000,,Moje ciało jest tutaj dowodem!
Dialogue: 0,0:17:18.97,0:17:22.33,NORMAL,,0000,0000,0000,,Tak by było,\Ngdybyś był prawdziwym Kisuke.
Dialogue: 0,0:17:23.80,0:17:26.53,NORMAL,,0000,0000,0000,,Otrzymałem raport,\Nże Kisuke zmarł kilka chwil temu.
Dialogue: 0,0:17:26.60,0:17:28.37,NORMAL,,0000,0000,0000,,- Sprawdźcie to!\N- Tak jest!
Dialogue: 0,0:17:31.03,0:17:32.13,NORMAL,,0000,0000,0000,,Udało mu się wywinąć?
Dialogue: 0,0:17:32.57,0:17:33.43,NORMAL,,0000,0000,0000,,Szlag!
Dialogue: 0,0:17:36.80,0:17:37.57,NORMAL,,0000,0000,0000,,Co?
Dialogue: 0,0:17:38.53,0:17:40.17,NORMAL,,0000,0000,0000,,To był zwykły oszust!
Dialogue: 0,0:17:40.23,0:17:41.27,NORMAL,,0000,0000,0000,,Aresztować go!
Dialogue: 0,0:17:51.83,0:17:52.93,NORMAL,,0000,0000,0000,,Zgubiłem go.
Dialogue: 0,0:17:58.77,0:18:00.30,NORMAL,,0000,0000,0000,,Kim jesteś?
Dialogue: 0,0:18:01.20,0:18:05.47,NORMAL,,0000,0000,0000,,Skoro znasz o mnie prawdę,\Nmusisz być przyjacielem Kisuke.
Dialogue: 0,0:18:05.93,0:18:08.20,NORMAL,,0000,0000,0000,,To oznacza,\Nże również musisz zginąć.
Dialogue: 0,0:18:09.47,0:18:12.53,NORMAL,,0000,0000,0000,,Więc to jednak prawda,\Nże zabiłeś Kisuke i jego żonę.
Dialogue: 0,0:18:13.07,0:18:14.20,NORMAL,,0000,0000,0000,,Coś ty za jeden?
Dialogue: 0,0:18:14.87,0:18:16.00,NORMAL,,0000,0000,0000,,Czy tak było?
Dialogue: 0,0:18:16.33,0:18:17.70,NORMAL,,0000,0000,0000,,I co z tego?
Dialogue: 0,0:18:18.53,0:18:19.83,NORMAL,,0000,0000,0000,,Nie wybaczę ci!
Dialogue: 0,0:18:21.40,0:18:23.87,NORMAL,,0000,0000,0000,,Więc mówiłeś prawdę, Naruto.
Dialogue: 0,0:18:24.90,0:18:25.73,NORMAL,,0000,0000,0000,,UWOLNIENIE!
Dialogue: 0,0:18:29.03,0:18:29.97,NORMAL,,0000,0000,0000,,Kakashi?!
Dialogue: 0,0:18:30.10,0:18:33.93,NORMAL,,0000,0000,0000,,To jeden z moich podwładnych.
Dialogue: 0,0:18:34.63,0:18:40.37,NORMAL,,0000,0000,0000,,{\fscx95}Teraz wyśpiewasz nam, co zamierzałeś zrobić\Npo przejęciu kontroli nad systemem obronnym wioski.
Dialogue: 0,0:18:41.57,0:18:42.83,NORMAL,,0000,0000,0000,,To koniec, Sabiru!
Dialogue: 0,0:18:43.07,0:18:44.43,NORMAL,,0000,0000,0000,,Proszę poczekać,\Nmistrzu Kakashi!
Dialogue: 0,0:18:45.10,0:18:48.00,NORMAL,,0000,0000,0000,,Może się pan teraz zemścić!
Dialogue: 0,0:18:49.60,0:18:50.90,NORMAL,,0000,0000,0000,,TRANSFORMACJA!
Dialogue: 0,0:18:53.23,0:18:54.70,NORMAL,,0000,0000,0000,,Sabiru, gotuj się na śmierć!
Dialogue: 0,0:18:54.90,0:18:56.70,NORMAL,,0000,0000,0000,,WIELOKROTNY PODZIAŁ CIENIA!
Dialogue: 0,0:18:59.00,0:18:59.90,NORMAL,,0000,0000,0000,,Na niego!
Dialogue: 0,0:19:00.43,0:19:02.13,NORMAL,,0000,0000,0000,,Sabiru!
Dialogue: 0,0:19:02.57,0:19:03.87,NORMAL,,0000,0000,0000,,Zdychaj!
Dialogue: 0,0:19:23.37,0:19:24.17,NORMAL,,0000,0000,0000,,Co?
Dialogue: 0,0:19:31.13,0:19:32.23,NORMAL,,0000,0000,0000,,Już po nim.
Dialogue: 0,0:19:32.77,0:19:34.23,NORMAL,,0000,0000,0000,,Mistrzu Kakashi!
Dialogue: 0,0:19:36.37,0:19:37.63,NORMAL,,0000,0000,0000,,Został wyeliminowany.
Dialogue: 0,0:19:40.10,0:19:41.27,NORMAL,,0000,0000,0000,,Dziękuję, Naruto.
Dialogue: 0,0:19:42.00,0:19:44.23,NORMAL,,0000,0000,0000,,Uratowałeś Wioskę Liścia.
Dialogue: 0,0:19:44.73,0:19:45.97,NORMAL,,0000,0000,0000,,To nie tylko moja zasługa.
Dialogue: 0,0:19:46.23,0:19:48.67,NORMAL,,0000,0000,0000,,Przecież połączyliśmy siły.
Dialogue: 0,0:19:49.13,0:19:50.57,NORMAL,,0000,0000,0000,,Nie gadaj głupstw.
Dialogue: 0,0:19:53.17,0:19:57.57,NORMAL,,0000,0000,0000,,Jednak teraz jest pan\Nprawdziwym duchem.
Dialogue: 0,0:19:58.03,0:19:59.10,NORMAL,,0000,0000,0000,,Cieszę się.
Dialogue: 0,0:19:59.70,0:20:02.67,NORMAL,,0000,0000,0000,,Teraz mogę udać się w zaświaty\Ni żyć z moją żoną po wieki.
Dialogue: 0,0:20:04.07,0:20:06.77,NORMAL,,0000,0000,0000,,Mam do ciebie prośbę, Naruto.
Dialogue: 0,0:20:07.07,0:20:08.07,NORMAL,,0000,0000,0000,,O co chodzi?
Dialogue: 0,0:20:08.70,0:20:14.07,NORMAL,,0000,0000,0000,,Gdy sobie o mnie przypomnisz,\Nzanieś na nasz grób tulipany.
Dialogue: 0,0:20:14.83,0:20:16.27,NORMAL,,0000,0000,0000,,Moja żona je uwielbiała.
Dialogue: 0,0:20:17.57,0:20:18.33,NORMAL,,0000,0000,0000,,Oczywiście!
Dialogue: 0,0:20:20.13,0:20:22.70,NORMAL,,0000,0000,0000,,Na mnie już czas, Naruto.
Dialogue: 0,0:20:23.73,0:20:25.47,NORMAL,,0000,0000,0000,,Żyj tak,\Nabyś niczego nie żałował.
Dialogue: 0,0:20:27.23,0:20:28.13,NORMAL,,0000,0000,0000,,Proszę pana!
Dialogue: 0,0:20:29.37,0:20:31.27,NORMAL,,0000,0000,0000,,Tak zrobię, Kisuke!
Dialogue: 0,0:20:32.77,0:20:34.23,NORMAL,,0000,0000,0000,,Żegnaj!
Dialogue: 0,0:21:05.00,0:21:15.00,NORMAL,,0000,0000,0000,,Tłumaczenie endingu już wkrótce!
Dialogue: 0,0:22:36.80,0:22:41.43,NORMAL,,0000,0000,0000,,Cholera jasne, Sasuke,\Nco to ma znaczyć?
Dialogue: 0,0:22:41.57,0:22:42.67,NORMAL,,0000,0000,0000,,Nie mam pojęcia!
Dialogue: 0,0:22:42.70,0:22:45.27,NORMAL,,0000,0000,0000,,Poza tym to przez ciebie\Nznaleźliśmy się w takiej sytuacji!
Dialogue: 0,0:22:45.37,0:22:48.47,NORMAL,,0000,0000,0000,,Skończ chrzanić!\NTo przez ciebie złapali Sakurę!
Dialogue: 0,0:22:48.50,0:22:52.80,NORMAL,,0000,0000,0000,,Przymknij się! Gdy my się tutaj kłócimy,\Nszanse na przeżycie Sakury maleją!
Dialogue: 0,0:22:53.13,0:22:55.57,NORMAL,,0000,0000,0000,,W pojedynkę nic nie osiągniemy!
Dialogue: 0,0:22:56.00,0:23:00.17,NORMAL,,0000,0000,0000,,W następnym odcinku Naruto Shippuudena:\N"Najgorszy w historii wyścig na trzech nogach".
Dialogue: 0,0:23:00.53,0:23:02.47,NORMAL,,0000,0000,0000,,Gdy jesteś w pobliżu,\Ndzieją się same złe rzeczy.
